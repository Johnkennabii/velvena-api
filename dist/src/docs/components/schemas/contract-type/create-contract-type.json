{
  "CreateContractTypeRequest": {
    "type": "object",
    "properties": {
      "name": {
        "type": "string",
        "description": "The name of the new contract type",
        "example": "Location longue durée"
      }
    },
    "required": ["name"]
  },
  "ContractType": {
    "type": "object",
    "properties": {
      "id": {
        "type": "string",
        "description": "Unique identifier of the contract type",
        "example": "ct_7a2c9d51"
      },
      "name": {
        "type": "string",
        "description": "Contract type name",
        "example": "Location longue durée"
      },
      "created_by": {
        "type": ["string", "null"],
        "description": "UUID of the user who created this contract type",
        "example": "1a2b3c4d-5e6f-7890-abcd-ef1234567890"
      },
      "created_at": {
        "type": "string",
        "format": "date-time",
        "example": "2025-10-09T18:35:00.000Z"
      },
      "updated_at": {
        "type": ["string", "null"],
        "format": "date-time",
        "example": null
      },
      "deleted_at": {
        "type": ["string", "null"],
        "format": "date-time",
        "example": null
      }
    },
    "required": ["id", "name", "created_at"]
  },
  "CreateContractTypeResponse": {
    "type": "object",
    "properties": {
      "success": {
        "type": "boolean",
        "example": true
      },
      "data": {
        "$ref": "#/components/schemas/ContractType"
      }
    }
  }
}