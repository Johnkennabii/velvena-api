{
  "CreateProspectRequest": {
    "type": "object",
    "required": ["firstname", "lastname", "email"],
    "properties": {
      "firstname": {
        "type": "string",
        "example": "Sophie"
      },
      "lastname": {
        "type": "string",
        "example": "Martin"
      },
      "email": {
        "type": "string",
        "format": "email",
        "example": "sophie.martin@email.fr"
      },
      "phone": {
        "type": "string",
        "example": "+33612345678"
      },
      "birthday": {
        "type": "string",
        "format": "date",
        "example": "1990-05-15"
      },
      "country": {
        "type": "string",
        "example": "France"
      },
      "city": {
        "type": "string",
        "example": "Lyon"
      },
      "address": {
        "type": "string",
        "example": "15 Rue de la République"
      },
      "postal_code": {
        "type": "string",
        "example": "69002"
      },
      "status": {
        "type": "string",
        "example": "new"
      },
      "source": {
        "type": "string",
        "example": "website"
      },
      "notes": {
        "type": "string",
        "example": "Intéressée par les robes de soirée"
      }
    }
  },
  "CreateProspectResponse": {
    "type": "object",
    "properties": {
      "success": {
        "type": "boolean",
        "example": true
      },
      "data": {
        "$ref": "#/components/schemas/Prospect"
      }
    }
  }
}
